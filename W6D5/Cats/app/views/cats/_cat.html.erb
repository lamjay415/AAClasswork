<table>
<thead>
    <th><%= cat.name.upcase %></th>
</thead>
<tbody>
    <tr>
    <td>Birthday: <%= cat.birth_date %></td>
    </tr>
    <td>Color: <%= cat.color %></td>
    <tr>
    <td>Sex: <%= cat.sex %></td>
    </tr>
    <tr>
    <td>Description: <%= cat.description %></td>
    </tr>
</tbody>
</table>
<% if form == :show %>
    <a href = "<%= edit_cat_url %>"> Edit this Cat </a>
    <br>
    <a href = "<%= cats_url %>"> Back to Cats </a>
<% else %>
    <a href = "<%= cat_url(cat) %>"> Look at this cat </a>
<% end %>
<br>